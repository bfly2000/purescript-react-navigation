{
  "name": "HelloMobileNavigation",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "./node_modules/.bin/bower cache clean && ./node_modules/.bin/bower install",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "build:ios": "rimraf output && pulp build --to index.js",
    "watch:ios": "rimraf output && pulp -w build --to index.js",
    "build:android": "pulp build --to index.android.js",
    "watch:android": "pulp -w build --to index.android.js",
    "test": "jest"
  },
  "dependencies": {
    "react": "~15.4.2",
    "react-native": "0.42.0",
    "react-native-web": "bfly2000/react-native-web#missing-navigation-components",
    "react-navigation": "^1.0.0-beta.7"
  },
  "devDependencies": {
    "babel-jest": "19.0.0",
    "babel-loader": "^6.4.1",
    "babel-preset-react-native": "1.9.1",
    "bower": "^1.8.0",
    "jest": "19.0.2",
    "pulp": "^10.0.3",
    "purescript": "^0.10.7",
    "purs-loader": "^2.4.2",
    "react-test-renderer": "~15.4.2",
    "rimraf": "^2.6.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
